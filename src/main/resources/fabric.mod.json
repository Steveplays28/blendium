{
  "schemaVersion": 1,
  "id": "blendium",
  "version": "${version}",
  "name": "Blendium",
  "description": "Alpha blending for Distant Horizons and Sodium!",
  "authors": [
    "Steveplays28",
    "null511"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/blendium",
    "sources": "https://github.com/Steveplays28/blendium",
    "issues": "https://github.com/Steveplays28/blendium/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/blendium/icon.png",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/KbWxgGg"
      }
    },
    "mc-publish": {
      "loaders": [
        "fabric",
        "quilt"
      ],
      "curseforge": 940986,
      "modrinth": "RE5cje8G"
    }
  },
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.steveplays28.blendium.client.BlendiumClient"
    ],
    "modmenu": [
      "io.github.steveplays28.blendium.client.compat.modmenu.BlendiumModMenuCompat"
    ]
  },
  "mixins": [
    "blendium.mixins.json"
  ],
  "accessWidener": "blendium.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${supported_minecraft_version}",
    "sodium": ">=0.5.2"
  },
  "suggests": {
    "distant-horizons": [
      ">=2.0.1",
      "2.0.1-a-dev",
      "2.0.2-a-dev"
    ],
    "iris": ">=1.6.0"
  }
}
